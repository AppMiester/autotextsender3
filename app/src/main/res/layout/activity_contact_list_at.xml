<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg"
    android:orientation="vertical"
    tools:context=".ContactListAT">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarcreate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f5f5f5"
        android:isScrollContainer="false"
        android:minHeight="?attr/actionBarSize"
        app:navigationIcon="@drawable/ic_perm_contact_calendar_black_24dp"
        app:subtitle="@string/contact_list"

        app:title="@string/auto_text" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listview"></ListView>





</LinearLayout>